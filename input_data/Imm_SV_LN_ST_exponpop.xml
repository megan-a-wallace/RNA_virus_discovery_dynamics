		<frequencyModel id="host_species.root.frequencyModel" normalize="true">
			<generalDataType idref="host_species.dataType"/>
			<frequencies>
				<parameter id="host_species.root.frequencies" dimension="2"/>
			</frequencies>
		</frequencyModel>
	</ancestralTreeLikelihood>

	<!-- END Discrete Traits Model                                               -->


	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="log">
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.clock.rate"/>
		</scaleOperator>
		<upDownOperator scaleFactor="0.75" weight="3">
			<up>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</up>
			<down>
				<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.clock.rate"/>
			</down>
		</upDownOperator>
		<deltaExchange delta="0.01" weight="3">
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.allNus"/>
		</deltaExchange>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.clock.rate"/>
		</scaleOperator>
		<upDownOperator scaleFactor="0.75" weight="3">
			<up>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</up>
			<down>
				<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.clock.rate"/>
			</down>
		</upDownOperator>
		<deltaExchange delta="0.01" weight="3">
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.allNus"/>
		</deltaExchange>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="host_species.clock.rate"/>
		</scaleOperator>
		<upDownOperator scaleFactor="0.75" weight="3">
			<up>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</up>
			<down>
				<parameter idref="host_species.clock.rate"/>
			</down>
		</upDownOperator>
		<subtreeSlide size="1.0" gaussian="true" weight="30">
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="30">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="3">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="3">
			<treeModel idref="treeModel"/>
		</wilsonBalding>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="30">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="exponential.popSize"/>
		</scaleOperator>
		<randomWalkOperator windowSize="1.0" weight="3">
			<parameter idref="exponential.growthRate"/>
		</randomWalkOperator>
		<scaleOperator scaleFactor="0.75" weight="15" scaleAllIndependently="true">
			<parameter idref="host_species.rates"/>
		</scaleOperator>
		<deltaExchange delta="0.75" weight="1">
			<parameter idref="host_species.root.frequencies"/>
		</deltaExchange>
	</operators>
	

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="100000000" autoOptimize="true">
		<joint id="joint">
			<prior id="prior">
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.kappa"/>
				</logNormalPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.alpha"/>
				</exponentialPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.alpha"/>
				</exponentialPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.kappa"/>
				</logNormalPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.alpha"/>
				</exponentialPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.alpha"/>
				</exponentialPrior>
				<dirichletPrior alpha="1.0" sumsTo="1.0">
					<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.allNus"/>
				</dirichletPrior>
				<logNormalPrior mean="5.0E-4" stdev="2.0E-4" offset="0.0">
					<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.clock.rate"/>
				</logNormalPrior>
				<dirichletPrior alpha="1.0" sumsTo="1.0">
					<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.allNus"/>
				</dirichletPrior>
				<logNormalPrior mean="5.0E-4" stdev="2.0E-4" offset="0.0">
					<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.clock.rate"/>
				</logNormalPrior>
				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="host_species.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>
				<oneOnXPrior>
					<parameter idref="exponential.popSize"/>
				</oneOnXPrior>
				<laplacePrior mean="1.0" scale="1.0">
					<parameter idref="exponential.growthRate"/>
				</laplacePrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="host_species.frequencies"/>
				</uniformPrior>
				<cachedPrior>
					<gammaPrior shape="1.0" scale="1.0" offset="0.0">
						<parameter idref="host_species.rates"/>
					</gammaPrior>
					<parameter idref="host_species.rates"/>
				</cachedPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="host_species.root.frequencies"/>
				</uniformPrior>
				<coalescentLikelihood idref="coalescent"/>
				
				
				<strictClockBranchRates idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.branchRates"/>
				<strictClockBranchRates idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.branchRates"/>
				<strictClockBranchRates idref="host_species.branchRates"/>
			</prior>
			<likelihood id="likelihood">
				<treeDataLikelihood idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.treeLikelihood"/>
				<treeDataLikelihood idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.treeLikelihood"/>

				<!-- START Discrete Traits Model                                             -->
				<ancestralTreeLikelihood idref="host_species.treeLikelihood"/>

				<!-- END Discrete Traits Model                                               -->

			</likelihood>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="1000000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="age(root)" sf="6" width="12">
				<tmrcaStatistic idref="age(root)"/>
			</column>
			<column label="Imm_SV_L_F_12seqs_trimmed_inframe.aln.clock.rate" sf="6" width="12">
				<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.clock.rate"/>
			</column>
			<column label="Imm_SV_N_F_12seqs_trimmed_inframe.aln.clock.rate" sf="6" width="12">
				<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.clock.rate"/>
			</column>
			<column label="host_species.clock.rate" sf="6" width="12">
				<parameter idref="host_species.clock.rate"/>
			</column>

			<!-- START Discrete Traits Model                                             -->

			<!-- END Discrete Traits Model                                               -->

		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="10000" fileName="Imm_SV_LN_ST_exponpop.log.txt" overwrite="false">
			<joint idref="joint"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<tmrcaStatistic idref="age(root)"/>
			<treeLengthStatistic idref="treeLength"/>
			<parameter idref="exponential.popSize"/>
			<parameter idref="exponential.growthRate"/>
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.kappa"/>
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.kappa"/>
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.alpha"/>
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.alpha"/>
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.kappa"/>
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.kappa"/>
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.alpha"/>
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.alpha"/>
			<compoundParameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.allNus"/>
			<statistic idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP1+2.mu"/>
			<statistic idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.CP3.mu"/>
			<parameter idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.clock.rate"/>
			<compoundParameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.allNus"/>
			<statistic idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP1+2.mu"/>
			<statistic idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.CP3.mu"/>
			<parameter idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.clock.rate"/>
			<parameter idref="host_species.clock.rate"/>
			<rateStatistic idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.meanRate"/>
			<rateStatistic idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.meanRate"/>
			<rateStatistic idref="host_species.meanRate"/>

			<!-- START Discrete Traits Model                                             -->
			<parameter idref="host_species.rates"/>

			<!-- END Discrete Traits Model                                               -->

			<treeDataLikelihood idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.treeLikelihood"/>
			<treeDataLikelihood idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.treeLikelihood"/>
			<strictClockBranchRates idref="Imm_SV_L_F_12seqs_trimmed_inframe.aln.branchRates"/>
			<strictClockBranchRates idref="Imm_SV_N_F_12seqs_trimmed_inframe.aln.branchRates"/>
			<strictClockBranchRates idref="host_species.branchRates"/>

			<!-- START Discrete Traits Model                                             -->
			<ancestralTreeLikelihood idref="host_species.treeLikelihood"/>

			<!-- END Discrete Traits Model                                               -->

			<coalescentLikelihood idref="coalescent"/>
			
		</log>

		<!-- START Discrete Traits Model                                             -->
		<log id="Imm_SV_LN_ST_exponpop.host_speciesrateMatrixLog" logEvery="10000" fileName="Imm_SV_LN_ST_exponpop.host_species.rates.log">
			<parameter idref="host_species.rates"/>
		</log>

		<!-- END Discrete Traits Model                                               -->


		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="10000" nexusFormat="true" fileName="Imm_SV_LN_ST_exponpop.trees.txt" sortTranslationTable="true">
